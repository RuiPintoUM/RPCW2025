ns1:Engenharia_Eletrotecnica_e_de_Computadores a ns1:Mestrado ;
    ns1:pertenceA ns1:N_A,
        ns1:Universidade_de_Tras_os_Montes_e_Alto_Douro ;
    ns1:temAreaCientifica ns1:Eletricidade_e_energia ;
